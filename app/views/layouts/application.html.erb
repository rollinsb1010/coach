<html>
<head>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
  
  <meta name="description" content="...">
  <meta name="keywords" content="...">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  
   <meta http-equiv="X-UA-Compatible" content="IE=edge">
   <meta http-equiv="Content-Type" content="text/html;charset=utf-8">
    
  <link href="http://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700,800" rel="stylesheet" type="text/css">  
  <link href="http://fonts.googleapis.com/css?family=Titillium+Web:300,400,600,700" rel="stylesheet" type="text/css">
  
  <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
</head>  
<body>	

	<!-- Header -->
  <header id="header">
    <hr class="hr_t">

		<nav class="navbar">
			<div class="navbar-inner">
				<div class="container">
        <!-- Logo -->
       <div id="coach_logo">
        <span style="float:left; width:70%; height:70%;"><%= link_to image_tag('Travel-Bus-256.png'), 'http://www.google.com' %></div><br />
        	<div id="coach_title"><h2>Homeless Coach</h2></span></div>
        <span style="float:right;">
        	<div id="user_nav">
        		  <% if current_user %>
        		    Logged in as <strong><%= current_user.email %></strong>.<br />
      
        		  <%= link_to "Logout", logout_path, method: :delete %>
        		  <% else %>
        		  <%= link_to "Sign Up", signup_path %><br />
        		  <%= link_to "Login", login_path %><br />
      			  <% end %>
            </div></span>
        
					<ul class="nav">
						<li><%= link_to 'Home', root_path %></li>
    					<li><%= link_to 'About', about_site_path %></li>
    					<li><%= link_to 'Support', donate_site_path %></li>
    					<li><%= link_to 'Heroes', portfolio_site_path %></li>
						<li><%= link_to 'Contact', contact_site_path %></li>
						<li><%= link_to 'Chat', messages_path %></li>
					</ul>
				</div>
			</div>
		</nav>
                        
  </header>
 


<%= yield %>

</body>
</html>
